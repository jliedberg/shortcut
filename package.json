{
  "name": "shortcut",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
    "dev": "concurrently --kill-others \"npm run dev-client\" \"npm run dev-server\" \"npm run dev-api\"",
    "dev-client": "NODE_ENV=development babel-node ./webpack/webpack-dev-server.js",
    "dev-server": "NODE_ENV=development nodemon --exec babel-node ./server/index.js",
    "dev-api": "NODE_ENV=development nodemon --exec babel-node ./server/api.js",
    "start": "concurrently --kill-others \"npm run start-api\" \"npm run start-server\"",
    "start-api": "NODE_ENV=production babel-node ./server/api.js",
    "start-server": "NODE_ENV=production babel-node ./server/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "nodemon": "^1.10.2",
    "webpack-dev-middleware": "^1.8.3",
    "webpack-hot-middleware": "^2.12.2",
    "sass-loader": "^4.0.2",
    "serialize-javascript": "^1.3.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "node-sass": "^3.13.0",
    "pretty-error": "^2.0.1",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.16.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "concurrently": "^3.1.0",
    "css-loader": "^0.25.0"
  },
  "dependencies": {
    "babel-cli": "^6.16.0",
    "body-parser": "^1.15.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.14.0",
    "mongoose": "^4.7.1",
    "normalize.css": "^5.0.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.6",
    "redux": "^3.6.0",
    "sigrid": "^1.0.0",
    "webpack": "^1.13.2",
    "webpack-isomorphic-tools": "^2.5.8"
  }
}
